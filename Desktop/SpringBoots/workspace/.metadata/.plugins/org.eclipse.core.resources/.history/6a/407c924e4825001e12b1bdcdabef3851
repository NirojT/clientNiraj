package Kanchanjunga.Services;

import java.util.List;
import java.util.UUID;

import org.springframework.web.multipart.MultipartFile;

import Kanchanjunga.Entity.FoodMenu;

public interface FoodMenuService {

	Boolean createFoodMenu(FoodMenu foodMenu);
	
	Boolean updateFoodMenu(UUID id, String name, String price, String category, String description,
			MultipartFile image,String imageName);

	Boolean deleteFoodMenu(UUID id);

	List<FoodMenu> getAllFoodMenu();

	FoodMenu getFoodMenuByID(UUID id);
	
	
}
